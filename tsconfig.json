{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@consts": ["src/consts"],
      "@favicon": ["src/favicon"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
  },
}
