{
  "name": "blog",
  "type": "module",
  "version": "2.0.0",
  "author": "Tugrul Ates",
  "license": "MIT",
  "homepage": "https://github.com/tugrulates/blog#readme",
  "bugs": {
    "url": "https://github.com/tugrulates/blog/issues"
  },
  "scripts": {
    "fetch-photos": "git submodule update --init --remote src/images/photos",
    "fetch": "npm run fetch-photos",
    "build-photos": "tsx src/scripts/photos.ts",
    "prebuild": "npm run fetch && npm run build-photos",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "tsx": "tsx"
  },
  "dependencies": {
    "@astrojs/check": "^0.3.1",
    "@astrojs/preact": "^3.0.1",
    "@astrojs/react": "^3.0.4",
    "@astrojs/sitemap": "^3.0.3",
    "@astrojs/tailwind": "^5.0.2",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@tailwindcss/typography": "^0.5.10",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "astro-remark-description": "^1.1.1",
    "exif": "^0.6.0",
    "exiftool-vendored": "^23.7.0",
    "glob": "^10.3.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.5",
    "tsx": "^4.6.2",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "^3.2.2",
    "@types/exif": "^0.6.5",
    "prettier": "^3.1.0",
    "prettier-plugin-astro": "^0.12.2",
    "prettier-plugin-tailwindcss": "^0.5.7"
  },
  "prettier": {
    "plugins": [
      "@prettier/plugin-xml",
      "prettier-plugin-astro",
      "prettier-plugin-tailwindcss"
    ],
    "xmlWhitespaceSensitivity": "ignore"
  }
}
