{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@arrays": ["src/arrays"],
      "@consts": ["src/consts"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
