{
  "name": "www",
  "type": "module",
  "version": "2.0.0",
  "author": "Tugrul Ates",
  "license": "MIT",
  "homepage": "https://github.com/tugrulates/www#readme",
  "bugs": {
    "url": "https://github.com/tugrulates/www/issues"
  },
  "scripts": {
    "fetch:blog": "git submodule update --init --remote src/blog",
    "fetch:photos": "git submodule update --init --remote src/photos",
    "fetch": "npm run fetch:blog && npm run fetch:photos",
    "build:blog": "tsx src/scripts/build-blog.ts",
    "build:photos": "tsx src/scripts/build-photos.ts",
    "prebuild": "npm run fetch && npm run build:blog && npm run build:photos",
    "lint": "npx eslint . && npx prettier -c .",
    "format": "npx prettier -cw .",
    "dev": "DRAFTS=true astro dev",
    "build": "npm run lint && astro check --minimumFailingSeverity=warning && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "tsx": "tsx"
  },
  "dependencies": {
    "@astro-community/astro-embed-youtube": "^0.4.3",
    "@astrojs/check": "^0.4.1",
    "@astrojs/mdx": "^2.1.1",
    "@astrojs/preact": "^3.1.0",
    "@astrojs/react": "^3.0.9",
    "@astrojs/rss": "^4.0.4",
    "@astrojs/sitemap": "^3.0.5",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/vercel": "^7.1.1",
    "@fontsource/fira-code": "^5.0.16",
    "@fontsource/fira-sans": "^5.0.18",
    "@headlessui/react": "^1.7.17",
    "@iconify-json/bi": "^1.1.23",
    "@iconify-json/heroicons": "^1.1.19",
    "@tailwindcss/typography": "^0.5.10",
    "@types/react": "^18.2.54",
    "@types/react-dom": "^18.2.15",
    "@vercel/analytics": "^1.1.3",
    "@vercel/speed-insights": "^1.0.9",
    "astro": "^4.3.2",
    "astro-icon": "^1.1.0",
    "astro-robots-txt": "^1.0.0",
    "astro-seo": "^0.8.0",
    "exif": "^0.6.0",
    "exiftool-vendored": "^24.4.0",
    "glob": "^10.3.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-external-links": "^3.0.0",
    "sharp": "^0.33.2",
    "sharp-ico": "^0.1.5",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.6.2"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "^3.2.2",
    "@types/exif": "^0.6.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "typescript": "^5.3.3"
  }
}
