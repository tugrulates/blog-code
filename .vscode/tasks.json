{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "server",
      "type": "shell",
      "command": "npm run dev",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "isBackground": true,
      "presentation": {
        "clear": true,
        "reveal": "silent"
      },
      "problemMatcher": {
        "background": {
          "beginsPattern": "Setting up local development server",
          "endsPattern": "Server now ready"
        },
        "pattern": [
          {
            "code": 4,
            "column": 3,
            "file": 1,
            "line": 2,
            "message": 5,
            "regexp": "^(.+):(\\d+):(\\d+): (\\w\\d+) (.+)$"
          }
        ]
      }
    }
  ]
}
